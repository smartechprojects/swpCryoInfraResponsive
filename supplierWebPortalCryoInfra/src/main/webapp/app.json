{
    "name": "SupplierApp",
    "version": "1.0.0.0",
    "indexHtmlPath": "index.html",
    "classpath": [
        "app"
    ],
    "overrides": [
        "overrides"
    ],
    "framework": "ext",
    "toolkit": "classic",
    "theme": "theme-crisp",
    "requires": [
        "font-awesome"
    ],
    "fashion": {
        "inliner": {
            "enable": false
        }
    },
    "sass": {
        "namespace": "SupplierApp",
        "generated": {
            "var": "sass/save.scss",
            "src": "sass/save"
        },
        "etc": [
            "sass/etc/all.scss"
        ],
        "var": [
            "sass/var/all.scss",
            "sass/var"
        ],
        "src": [
            "sass/src"
        ]
    },
    "js": [
        {

        },
        {
            "path": "app.js",
            "bundle": true
        }
    ],
    "css": [
        {
            "path": "${build.out.css.path}",
            "bundle": true,
            "exclude": ["fashion"]
        },
		{
		 "path": "resources/css/app.css"
		}
    ],
     "loader": {
         "cache": false,
         "cacheParam": "_dc"
     },
    "production": {
        "output": {
            "appCache": {
                "enable": true,
                "path": "cache.appcache"
            }
        },
        "loader": {
            "cache": "${build.timestamp}"
        },
        "cache": {
            "enable": true
        },
        "compressor": {
            "type": "yui"
        }
    },
    "testing": {
    },
    "development": {
        "watch": {
            "delay": 250
        }
    },
    "bootstrap": {
        "base": "${app.dir}",
        
        "microloader": "bootstrap.js",
        "css": "bootstrap.css"
    },
    "output": {
        "base": "${workspace.build.dir}/${build.environment}/${app.name}",
        "appCache": {
            "enable": false
        }
    },
    "cache": {
        "enable": false,
        "deltas": true
    },
    "appCache": {
        "cache": [
            "index.html"
        ],
        "network": [
            "*"
        ],
        "fallback": []
    },
    "resources": [
        {
            "path": "resources",
            "output": "shared"
        }
    ],
    "archivePath": "archive",
    "slicer": {
        "js": [
            {
                "path": "sass/example/custom.js",
                "isWidgetManifest": true
            }
        ],
        "output": {
            "appCache": {
                "enable": false
            }
        },
        "cache": {
            "enable": false
        }
    },
    "ignore": [
        "(^|/)CVS(/?$|/.*?$)"
    ],
    "id": "85ffb9a1-f2a6-42b4-8a0d-c11473150fc5"
}
